modules = ["nodejs-20", "web", "postgresql-16", "gcloud", "python-base-3.13"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-24_05"
packages = ["python312Packages.python-lsp-black", "sbclPackages.parse-float"]

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34287
externalPort = 3001

[[ports]]
localPort = 42741
externalPort = 3000

[env]
PORT = "5000"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[[workflows.workflow]]
name = "Autoflow"
mode = "parallel"
author = 46911784

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[agent]
integrations = [
  "javascript_sendgrid:1.0.0",
  "javascript_database:1.0.0",
  "javascript_log_in_with_replit:1.0.0",
  "notion:1.0.0",
]
